+            byte[] bytes = new byte[data.size()];
+
+            for (int i = 0; i < bytes.length; i++) {
+                bytes[i] = data.get(i);
+            }
+
+            String text = new String(bytes);
+            return Integer.valueOf(text);
+        } catch (Exception ex) { //datafile not found
